{
  "name": "Export FlowJo",
  "description": "Exports FlowJo results.",
  "tags": [
    "export"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/flowjo_export_operator"
  ],
  "properties": [
    {
      "kind": "StringProperty",
      "name": "output_folder",
      "defaultValue": "Exported data"
    },
    {
      "kind": "DoubleProperty",
      "name": "character_na_value",
      "defaultValue": 0,
      "description": "Numeric value for NA values in character variables"
    },
    {
      "kind": "DoubleProperty",
      "name": "integer_na_value",
      "defaultValue": 0,
      "description": "Numeric value for NA values in integer variables"
    },
    {
      "kind": "DoubleProperty",
      "name": "double_na_value",
      "defaultValue": 0,
      "description": "Numeric value for NA values in double variables"
    }
  ],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "1.0.0",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [
          {
            "kind": "MetaFactor",
            "name": "filename",
            "type": "",
            "description": "Filename for grouping exported data (required)",
            "ontologyMapping": "document",
            "crosstabMapping": "row",
            "cardinality": "1",
            "factors": []
          },
          {
            "kind": "MetaFactor",
            "name": "rowId",
            "type": "",
            "description": "Row identifier (required)",
            "ontologyMapping": "observation",
            "crosstabMapping": "row",
            "cardinality": "1",
            "factors": []
          },
          {
            "kind": "MetaFactor",
            "name": "Results",
            "type": "",
            "description": "Result columns to export",
            "ontologyMapping": "variable",
            "crosstabMapping": "row",
            "cardinality": "1..n",
            "factors": []
          }
        ],
        "axis": []
      }
    ],
    "outputSpecsV2": []
  }
}
